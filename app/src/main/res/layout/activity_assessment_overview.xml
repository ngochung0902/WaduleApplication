<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_background">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ll_group_bottom"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/boder_bg_content">
        <TextView
            android:text="ASSESSMENT HISTORY"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color_white"
            android:textSize="@dimen/font14"
            android:drawableLeft="@drawable/ic_newuser"
            android:padding="@dimen/margin10"
            android:gravity="center_vertical"
            android:drawablePadding="@dimen/margin5"
            android:layout_marginLeft="@dimen/margin5"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lbTitle" />

        <LinearLayout
            android:id="@+id/ll_group_center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:background="@drawable/border_rect_layout"
            >
            <RelativeLayout
                android:id="@+id/rl_header1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin5">

                <TextView
                    android:id="@+id/title_part1"
                    android:text="PART 1: Reading Aloud"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:textSize="@dimen/font14"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/margin10"
                    android:gravity="left|center_vertical"
                    android:layout_gravity="center_horizontal"
                    />

                <TextView
                    android:text="VIEW TEXT  "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:drawableLeft="@drawable/ic_textview"
                    android:textSize="@dimen/font12"
                    android:layout_marginRight="@dimen/margin10"
                    android:layout_alignParentRight="true"
                    android:gravity="center"/>
            </RelativeLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#57798f83">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tvTime1"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:text=""
                        android:gravity="center"
                        android:textSize="@dimen/font14"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        android:visibility="invisible"/>
                    <ImageView
                        android:id="@+id/btnAudioP1"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:src="@drawable/btn_play"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_score1"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/margin20"
                    android:layout_marginRight="@dimen/margin10"
                    android:background="@color/color_dark_green_bg">
                    <TextView
                        android:id="@+id/btnScore1"
                        android:text="SCORE"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:layout_centerVertical="true"
                        android:padding="@dimen/margin5"
                        android:layout_toLeftOf="@+id/scorePart1"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        />
                    <TextView
                        android:id="@+id/scorePart1"
                        android:text="85"
                        android:layout_width="50dp"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:gravity="center"
                        android:padding="@dimen/margin5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@color/color_dark_green_bg"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>

            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rl_header2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin15">

                <TextView
                    android:id="@+id/title_part2"
                    android:text="PART 2: Talk about this picture"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:textSize="@dimen/font14"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/margin10"
                    android:gravity="left|center_vertical"
                    android:layout_gravity="center_horizontal"
                    />

                <TextView
                    android:text="VIEW PHOTO"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:drawableLeft="@drawable/ic_textview"
                    android:textSize="@dimen/font12"
                    android:layout_marginRight="@dimen/margin10"
                    android:layout_alignParentRight="true"
                    android:gravity="center"/>
            </RelativeLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#57798f83">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tvTime2"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:text="1:20:36"
                        android:gravity="center"
                        android:textSize="@dimen/font14"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        android:visibility="invisible"/>
                    <ImageView
                        android:id="@+id/btnAudioP2"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:src="@drawable/btn_play"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_score2"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/margin20"
                    android:layout_marginRight="@dimen/margin10"
                    android:background="@color/color_dark_green_bg">
                    <TextView
                        android:id="@+id/btnScore2"
                        android:text="SCORE"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:layout_centerVertical="true"
                        android:padding="@dimen/margin5"
                        android:layout_toLeftOf="@+id/scorePart2"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        />
                    <TextView
                        android:id="@+id/scorePart2"
                        android:text="85"
                        android:layout_width="50dp"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:gravity="center"
                        android:padding="@dimen/margin5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@color/color_dark_green_bg"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_header3"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin15">

                <TextView
                    android:text="PART 3: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:textSize="@dimen/font14"
                    android:layout_marginLeft="@dimen/margin10"
                    android:gravity="left|center_vertical"
                    android:layout_gravity="center_horizontal"
                    />

                <TextView
                    android:id="@+id/title_part3"
                    android:text="Spoken Interaction | What is your hobby?"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:textSize="@dimen/font12"
                    android:layout_marginLeft="@dimen/margin5"
                    android:gravity="center"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#57798f83">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tvTime3"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:text="1:20:36"
                        android:gravity="center"
                        android:textSize="@dimen/font14"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        android:visibility="invisible"/>
                    <ImageView
                        android:id="@+id/btnAudioP3"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:src="@drawable/btn_play"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_score3"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/margin20"
                    android:layout_marginRight="@dimen/margin10"
                    android:background="@color/color_dark_green_bg">
                    <TextView
                        android:id="@+id/btnScore3"
                        android:text="SCORE"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:layout_centerVertical="true"
                        android:padding="@dimen/margin5"
                        android:layout_toLeftOf="@+id/scorePart3"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        />
                    <TextView
                        android:id="@+id/scorePart3"
                        android:text="85"
                        android:layout_width="50dp"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:gravity="center"
                        android:padding="@dimen/margin5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@color/color_dark_green_bg"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_header4"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin15">

                <TextView
                    android:text="PART 4: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:textSize="@dimen/font14"
                    android:layout_marginLeft="@dimen/margin10"
                    android:gravity="left|center_vertical"
                    android:layout_gravity="center_horizontal"
                    />

                <TextView
                    android:id="@+id/title_part4"
                    android:text="Spoken Interaction | What is your favorite book?"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_white"
                    android:textSize="@dimen/font12"
                    android:layout_marginLeft="@dimen/margin5"
                    android:gravity="center"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#57798f83"
                android:layout_marginBottom="@dimen/margin5">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tvTime4"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:text="1:20:36"
                        android:gravity="center"
                        android:textSize="@dimen/font14"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        android:visibility="invisible"/>
                    <ImageView
                        android:id="@+id/btnAudioP4"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:padding="@dimen/margin10"
                        android:src="@drawable/btn_play"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin20"
                        android:background="@drawable/bg_textfield_login"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_score4"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/margin20"
                    android:layout_marginRight="@dimen/margin10"
                    android:background="@color/color_dark_green_bg">
                    <TextView
                        android:id="@+id/btnScore4"
                        android:text="SCORE"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:layout_centerVertical="true"
                        android:padding="@dimen/margin5"
                        android:layout_toLeftOf="@+id/scorePart4"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        />
                    <TextView
                        android:id="@+id/scorePart4"
                        android:text="85"
                        android:layout_width="50dp"
                        android:layout_height="40dp"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/font14"
                        android:gravity="center"
                        android:padding="@dimen/margin5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@color/color_dark_green_bg"
                        android:layout_gravity="center_horizontal"
                        />
                </RelativeLayout>

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_group_bottom"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="left|center"
            android:paddingLeft="@dimen/margin20">

            <TextView
                android:text="Examinee ID:"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/lbExamId"
                android:textColor="@color/color_white"
                android:textSize="@dimen/font14"/>

            <TextView
                android:text="1536-AF-4581"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/tvExamId"
                android:textColor="@color/color_white"
                android:textSize="@dimen/font16"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:gravity="center">
            <TextView
                android:text="SUBMIT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvSubmit"
                android:textSize="@dimen/font16"
                android:textColor="@color/color_white"
                android:layout_gravity="center"
                android:paddingLeft="@dimen/margin10"
                android:paddingRight="@dimen/margin10"
                android:layout_marginTop="@dimen/margin15"
                android:layout_marginBottom="@dimen/margin15"
                android:background="@drawable/border_rect_green_dark"
                android:visibility="visible"/>
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>